<?xml version="1.0" encoding="ASCII"?>
<arduino:Project xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:arduino="http://www.obeo.fr/arduino">
  <board xsi:type="arduino:ArduinoBoard" name="DFRduino UNO R3">
    <digitalPins>
      <module xsi:type="arduino:PushButton" name="Push Button"/>
    </digitalPins>
    <digitalPins id="1">
      <module xsi:type="arduino:LED" name="White LED"/>
    </digitalPins>
  </board>
  <sketch name="Blinker">
    <block>
      <instructions xsi:type="arduino:If">
        <block>
          <instructions xsi:type="arduino:Delay" value="500"/>
          <instructions xsi:type="arduino:ModuleAssignment" module="//@board/@digitalPins.1/@module">
            <operand xsi:type="arduino:IntegerConstant" value="1023"/>
          </instructions>
          <instructions xsi:type="arduino:Delay" value="500"/>
          <instructions xsi:type="arduino:ModuleAssignment" module="//@board/@digitalPins.1/@module">
            <operand xsi:type="arduino:IntegerConstant"/>
          </instructions>
        </block>
        <condition xsi:type="arduino:BooleanModuleGet" module="//@board/@digitalPins.0/@module"/>
      </instructions>
    </block>
  </sketch>
</arduino:Project>
