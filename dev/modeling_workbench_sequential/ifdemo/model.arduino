<?xml version="1.0" encoding="ASCII"?>
<arduino:Project xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:arduino="http://www.obeo.fr/arduino">
  <hardware name="Hardware" platforms="//@platform.0" modules="//@modules.0 //@modules.1">
    <connectors pin="//@platform.0/@digitalPins.0" module="//@modules.0"/>
    <connectors pin="//@platform.0/@digitalPins.1" module="//@modules.1"/>
  </hardware>
  <sketch name="Sketch" next="//@sketch/@instructions.0" hardware="//@hardware">
    <instructions xsi:type="arduino:If" next="//@sketch">
      <instructions xsi:type="arduino:Repeat" iteration="2">
        <instructions xsi:type="arduino:ModuleAssignment" next="//@sketch/@instructions.0/@instructions.0/@instructions.1" module="//@modules.0">
          <operand xsi:type="arduino:IntegerConstant" value="1023"/>
        </instructions>
        <instructions xsi:type="arduino:Delay" next="//@sketch/@instructions.0/@instructions.0/@instructions.2" value="500"/>
        <instructions xsi:type="arduino:ModuleAssignment" next="//@sketch/@instructions.0/@instructions.0/@instructions.3" module="//@modules.0">
          <operand xsi:type="arduino:IntegerConstant"/>
        </instructions>
        <instructions xsi:type="arduino:Delay" value="500"/>
      </instructions>
      <condition xsi:type="arduino:BinaryBooleanExpression" operator="equal">
        <left xsi:type="arduino:BooleanModuleGet" module="//@modules.1"/>
        <right xsi:type="arduino:BooleanConstant" value="true"/>
      </condition>
    </instructions>
  </sketch>
  <modules xsi:type="arduino:LED" name="White LED" level="true"/>
  <modules xsi:type="arduino:PushButton" name="Push button 1" level="true"/>
  <platform name="DFRduino UNO R3">
    <digitalPins/>
    <digitalPins id="1"/>
  </platform>
</arduino:Project>
